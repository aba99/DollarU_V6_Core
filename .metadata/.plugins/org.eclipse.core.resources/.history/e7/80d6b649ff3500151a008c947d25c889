package com.orsyp.tools.ps;


public class CopyOfTest {

	public static void main(String[] args) throws Exception {

		String fileName = args[0];
		String file_upr_dep=args[3];
		
		
		Connector conn = new Connector(fileName,true,"",true,"",true,"");
	
		
		InMemoryFile u_d = new InMemoryFile(file_upr_dep);
		u_d.store();
		
		
		
		for(String espKJobKey:u_d.getHash_Store().keySet())
		{
			

				conn.getConnectionList().get(0).setDepConOnUproc(espKJobKey, u_d.getHash_Store().get(espKJobKey));
		}
	}	
		
	

}
